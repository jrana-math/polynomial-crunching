f25 = sub(M, {a=>x_12, b=>x_13, c=>x_15, d=>x_22, e=>x_23, f=>x_25});
g7 = (det f23)*(det f24)*(det f10)*(det f16)-(det f25)*(det f3)*(det f22)*(det f20);
