g5 = (det f5)*(det f17)*(det f18)*(det f16)-(det f19)*(det f13)*(det f9)*(det f20);

f21 = sub(M, {a=>x_11, b=>x_13, c=>x_14, d=>x_21, e=>x_23, f=>x_24});
