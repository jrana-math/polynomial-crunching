f13 = sub(M, {a=>x_11, b=>x_14, c=>x_16, d=>x_21, e=>x_24, f=>x_26});
f14 = sub(M, {a=>x_13, b=>x_16, c=>x_17, d=>x_23, e=>x_26, f=>x_27});
